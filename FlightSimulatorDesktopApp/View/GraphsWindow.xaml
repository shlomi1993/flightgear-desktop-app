<Window x:Class="FlightSimulatorDesktopApp.View.GraphsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlightSimulatorDesktopApp.View"
        xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="Simulation Analysis" Height="768" Width="1024">
    <Grid Background="AliceBlue">
        <oxy:PlotView Height="300" Width="500" Model="{Binding HistoryModel}" Margin="6,23,518,425" BorderThickness="3" BorderBrush="Black"/>
        <oxy:PlotView Height="300" Width="500" Model="{Binding CorrelativeModel}" Margin="514,23,10,425" BorderThickness="3" BorderBrush="Black"/>
        <oxy:PlotView Height="380" Width="635" Model="{Binding LinRegModel}" Margin="162,354,162,14" BorderThickness="3" BorderBrush="Black"/>
    </Grid>
</Window>
