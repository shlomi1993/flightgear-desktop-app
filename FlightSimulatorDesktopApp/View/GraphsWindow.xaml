<Window x:Class="FlightSimulatorDesktopApp.View.GraphsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FlightSimulatorDesktopApp.Model" xmlns:oxy="http://oxyplot.org/wpf"
        mc:Ignorable="d"
        Title="GraphsWindow" Height="600" Width="1200">
    <Window.DataContext>
        <local:GraphsModel/>
    </Window.DataContext>
    <Grid>
        <oxy:PlotView Model="{Binding TimeModel}" Margin="10,10,674,292"/>
        <oxy:PlotView Model="{Binding CorrelativeModel}" Margin="698,10,10,292"/>
        <oxy:PlotView Model="{Binding LineRegModel}" Margin="354,292,354,10"/>
    </Grid>
</Window>
